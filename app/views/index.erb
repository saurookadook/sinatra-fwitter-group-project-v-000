<h1>Welcome to Fwitter</h1><br>

<p><a href="/signup">Sign Up!</a></p><br>
<p><a href="/login">Log In!</a></p><br>

<h2>Fweets!</h2>

<br></br>

<ul>
  <% @users.each do |user| %>
    <li><h3><%= user.username %></h3>
      <% @tweets.each do |tweet| %>
        <p><%= tweet.created_at %>: <%= tweet.content %></p>
      <% end %>
    </li><br>
  <% end %>
</ul>
